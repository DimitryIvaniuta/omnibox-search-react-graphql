{
  "name": "omnibox-search-react-graphql",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "devold": "node ./dev-with-links.mjs",
    "dev:web": "node ./dev-with-links.mjs",
    "dev:bff": "npm --prefix bff-search-analytics run dev",
    "dev": "concurrently -n web,bff -c green,blue \"npm run dev:web\" \"npm run dev:bff\"",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "codegen": "dotenv -e .env.codegen -- graphql-codegen --config codegen.yml",
    "precommit:guard": "git diff --cached --name-only | findstr /R \".*\\.(ts|tsx)$\" | findstr /V /R \"^$\" > files.tmp && (for /F %f in (files.tmp) do findstr /R \"graphql\\s*\\(\" %f && exit /B 1) || exit /B 0"
  },
  "dependencies": {
    "@apollo/client": "~3.14.0",
    "@graphql-typed-document-node/core": "^3.2.0",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "clsx": "^2.1.1",
    "decimal.js": "^10.6.0",
    "graphql": "^16.11.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
    "react-hook-form": "^7.66.0",
    "react-router-dom": "^6.30.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@graphql-codegen/cli": "^6.0.1",
    "@graphql-codegen/client-preset": "^5.1.1",
    "@graphql-codegen/fragment-matcher": "^6.0.0",
    "@graphql-codegen/introspection": "^5.0.0",
    "@graphql-codegen/typescript": "^5.0.2",
    "@graphql-codegen/typescript-operations": "^5.0.2",
    "@graphql-codegen/typescript-react-apollo": "^4.3.3",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "@vitejs/plugin-react": "^5.0.4",
    "@vitest/coverage-v8": "^4.0.6",
    "concurrently": "^9.2.1",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^11.0.0",
    "eslint": "^9.36.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.5.0",
    "jsdom": "^27.1.0",
    "sass": "^1.93.2",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.7",
    "vite-plugin-checker": "^0.11.0",
    "vitest": "^4.0.6"
  }
}
